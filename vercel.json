{
  "version": 2,
  "builds": [
    {
      "src": "App/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "App/app.py"
    }
  ],
  "functions": {
    "App/app.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "GROQ_API_KEY": "@groq_api_key"
  }
}